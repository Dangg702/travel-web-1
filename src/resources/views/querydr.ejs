<h3><%= title %></h3>
<div class="table-responsive">
  <form action="querydr" method="POST">
    <div class="form-group">
      <label for="orderId">Mã giao dịch (vnp_TxnRef)</label>
      <input
        type="text"
        class="form-control"
        name="orderId"
        placeholder="txnRef"
      />
    </div>
    <div class="form-group">
      <label for="transDate"
        >Thời gian tạo giao dịch (vnp_TransactionDate)</label
      >
      <input
        type="text"
        class="form-control"
        name="transDate"
        placeholder="yyyyMMddHHmmss"
      />
    </div>
    <button type="submit" class="btn btn-default">QueryDr</button>
  </form>
</div>
